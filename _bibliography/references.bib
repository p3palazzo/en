@article{palazzo:2015planaltina6,
  title = {Planaltina e suas narrativas: cultura, memória e patrimônio em publicações locais desde o Século XX},
  shorttitle = {Planaltina e suas narrativas},
  author = {Palazzo, Pedro P.},
  date = {2015},
  journaltitle = {Historiæ},
